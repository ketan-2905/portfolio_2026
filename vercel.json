{
  "headers": [
    {
      "source": "/download/(.*)\\.apk",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.android.package-archive"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=Dotspace.apk"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/download/:file*.apk",
      "destination": "/download/:file*.apk"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
