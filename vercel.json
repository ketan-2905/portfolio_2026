{
  "headers": [
    {
      "source": "/download/(.*)\\.apk",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/vnd.android.package-archive"
        },
        {
          "key": "Content-Disposition",
          "value": "attachment; filename=\"Dotspace.apk\""
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "^/download/(.*\\.apk)$",
      "dest": "/download/$1"
    },
    {
      "src": "^(?!/download/).*",
      "dest": "/index.html"
    }
  ]
}
